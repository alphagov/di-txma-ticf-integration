{
  "name": "di-txma-ticf-integration",
  "description": "System to automate the retrieval of audit data via Zendesk integration",
  "repository": {
    "type": "git",
    "url": "https://github.com/alphagov/di-txma-ticf-integration"
  },
  "author": "GDS",
  "license": "MIT",
  "packageManager": "yarn@3.2.2",
  "private": true,
  "scripts": {
    "build": "webpack",
    "lint": "prettier . --check ; eslint .",
    "lint:fix": "prettier . --write ; eslint . --fix",
    "postinstall": "husky install",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "test": "jest -c jest.config.ts",
    "test:cov": "jest -c jest.config.ts --coverage",
    "test:integration": "jest -c jest.integration.config.ts --runInBand"
  },
  "dependencies": {
    "@aws-sdk/client-athena": "3.163.0",
    "@aws-sdk/client-cloudformation": "3.163.0",
    "@aws-sdk/client-dynamodb": "3.163.0",
    "@aws-sdk/client-glue": "3.163.0",
    "@aws-sdk/client-s3": "3.163.0",
    "@aws-sdk/client-s3-control": "3.163.0",
    "@aws-sdk/client-secrets-manager": "3.163.0",
    "@aws-sdk/client-sqs": "3.170.0",
    "notifications-node-client": "5.1.1"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.171.0",
    "@jest/types": "28.1.3",
    "@types/aws-lambda": "8.10.102",
    "@types/jest": "28.1.7",
    "@types/jest-when": "3.5.2",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "aws-sdk-client-mock": "1.0.0",
    "aws-sdk-client-mock-jest": "^2.0.0",
    "aws-testing-library": "^2.1.5",
    "axios": "0.27.2",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.8.3",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-extended": "^3.1.0",
    "jest-when": "3.5.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "ts-jest": "28.0.8",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "yaml-cfn": "0.3.1"
  },
  "lint-staged": {
    "*": "prettier --write",
    "*.{js,ts}": "eslint --fix"
  }
}
